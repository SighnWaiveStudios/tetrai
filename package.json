    "@types/jest": "^29.4.2",
    "@types/node": "^15.12.2",
    "dotenv": "^10.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.3.2"
  }
}
